<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Version>0.20.0</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageId>MockWebApi.Configuration</PackageId>
    <Authors>Marco Ruether</Authors>
    <Company>mnemonic BIT GmbH</Company>
    <Description>Contracts for communicating with the MockWebApi service through the client library.</Description>
    <IsPackable>true</IsPackable>
    <Nullable>enable</Nullable>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="YamlDotNet" Version="15.3.0" />
  </ItemGroup>

</Project>
